{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "postgresql",
      "type": "cppdbg",
      "request": "attach",
      "program": "/home/tanboyu/cpp_workspace/qbase_data/bin/postgres",
      "processId": "${command:pickProcess}",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
